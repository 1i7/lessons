// запускается один раз при запуске контроллера
void setup() {
    pinMode(13, OUTPUT); // 13й пин режим вывода
}

// бесконечный цикл
void loop() {
    digitalWrite(13, 1); // включить лампочку
    delay(1000); // подождать секунду
    digitalWrite(13, 0); // выключить лампочку
    delay(1000); // подождать секунду
}


